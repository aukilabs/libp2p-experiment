namespace Libposemesh;

table GenericDomainData {
  name:string;
  data_type:string;
  data:[byte];
}

table Partition {
  data:[bool];
}

union AnyDomainData { GenericDomainData, Partition }

table DomainData {
  data_offset:uint32;
  id:string;
  domain_id:string;
  name:string;
  data:AnyDomainData;
}
